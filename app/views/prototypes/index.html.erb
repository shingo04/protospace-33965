<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <%= "こんにちは、" %>
        <%= link_to "#{@user.name}", user_path(@user), class: :greeting__link%>
        <%= "さん" %>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%= render partial: "prototype", collection: @prototypes %>
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
    </div>
  </div>
</main>
